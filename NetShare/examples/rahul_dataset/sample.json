{
    "global_config": {
        "overwrite": true,
        "dataset_type": "netflow",
        "n_chunks": 1,
        "dp": false,
        "original_data_file": "traces/dataset.csv"
    },
    "default": "single_event_per_row.json",
    "pre_post_processor": {
        "class": "NetsharePrePostProcessor",
        "config": {
            "word2vec": {
                "vec_size": 10,
                "model_name": "word2vec_vecSize",
                "annoy_n_trees": 100,
                "pretrain_model_path": null
            },
            "metadata": [
                {
                    "column": "IP__src_s",
                    "type": "integer",
                    "encoding": "bit",
                    "n_bits": 32,
                    "categorical_mapping": false
                },
                {
                    "column": "IP__dst_s",
                    "type": "integer",
                    "encoding": "bit",
                    "n_bits": 32,
                    "categorical_mapping": false
                },
                {
                    "column": "IP__p",
                    "type": "integer",
                    "encoding": "word2vec_proto"
                },
                {
                    "column": "IP__type",
                    "type": "integer",
                    "encoding": "bit",
                    "n_bits": 32,
                    "categorical_mapping": false
                },
                {
                    "column": "Label",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "packet__layers_Ethernet",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "packet__layers_IP",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "packet__layers_TCP",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "UDP__dport",
                    "type": "integer",
                    "encoding": "word2vec_port"
                },
                {
                    "column": "UDP__sport",
                    "type": "integer",
                    "encoding": "word2vec_port"
                },
                {
                    "column": "TCP__sport",
                    "type": "integer",
                    "encoding": "word2vec_port"
                },
                {
                    "column": "TCP__dport",
                    "type": "integer",
                    "encoding": "word2vec_port"
                }
            ],
            "timeseries": [
                {
                    "column": "IP__ttl",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "packet__len",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__answers_name",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "DNS__answers_type",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__answers_cls",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__answers_ttl",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__answers_dlen",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__answers_address",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "DNS__query",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__an",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__dlen",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__ttl",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__opcode",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "DNS__type",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "IEEE__type",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "IEEE__dsr",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "MQTT__mlen",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "TCP__flags",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "IP__len",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "TCP__seq",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                }
            ],
            "timestamp": {
                "column": "packet__time",
                "generation": true,
                "encoding": "interarrival",
                "normalization": "ZERO_ONE"
            }
        }
    },
    "model": {
        "class": "DoppelGANgerTorchModel",
        "config": {
            "batch_size": 100,
            "sample_len": [
                1,
                5,
                10
            ],
            "sample_len_expand": true,
            "epochs": 50,
            "extra_checkpoint_freq": 1,
            "epoch_checkpoint_freq": 5
        }
    }
}
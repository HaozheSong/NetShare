{
  "processors": {
    "preprocessors": [
      {
        "class": "CustomizableFormatPreprocessor",
        "config": {
          "input_file_format": "csv"
        }
      },
      {
        "class": "csv_pre_processor"
      }
    ],
    "postprocessors": [
      {
        "class": "csv_post_processor"
      }
    ]
  },
  "global_config": {
    "overwrite": true,
    "dataset_type": "netflow",
    "n_chunks": 1,
    "dp": false
  },
  "default": "single_event_per_row.json",
  "pre_post_processor": {
    "class": "NetsharePrePostProcessor",
    "config": {
      "word2vec": {
        "vec_size": 10,
        "model_name": "word2vec_vecSize",
        "annoy_n_trees": 100,
        "pretrain_model_path": null
      },
      "metadata": [],
      "timeseries": []
    }
  },
  "model": {
    "class": "DoppelGANgerTorchModel",
    "config": {
      "batch_size": 100,
      "sample_len": [
        1,
        5,
        10
      ],
      "sample_len_expand": true,
      "epochs": 50,
      "extra_checkpoint_freq": 1,
      "epoch_checkpoint_freq": 5
    }
  },
  "fields": {
    "timestamp": [
      {
        "name": "packet__time",
        "format": "timestamp",
        "encoding": "timestamp",
        "type": "unprocessed",
        "time_format": "%Y-%m-%d %H:%M:%S.%f"
      }
    ],
    "metadata": [
      {
        "name": "IP__src_s",
        "format": "IP",
        "encoding": "bit",
        "type": "IPv4"
      },
      {
        "name": "IP__dst_s",
        "format": "IP",
        "encoding": "bit",
        "type": "IPv4"
      },
      {
        "name": "IP__p",
        "format": "integer",
        "encoding": "word_proto"
      },
      {
        "name": "IP__type",
        "format": "integer",
        "abnormal": true,
        "encoding": "bit"
      },
      {
        "name": "Label",
        "format": "string",
        "encoding": "categorical"
      },
      {
        "name": "packet__layers",
        "format": "list",
        "encoding": "list_attributes",
        "names": [
          "Ethernet",
          "IP",
          "TCP"
        ],
        "delimiter": ","
      },
      {
        "name": "UDP__dport",
        "format": "integer",
        "abnormal": true,
        "encoding": "word_port"
      },
      {
        "name": "UDP__sport",
        "format": "integer",
        "abnormal": true,
        "encoding": "word_port"
      },
      {
        "name": "TCP__sport",
        "format": "integer",
        "encoding": "word_port"
      },
      {
        "name": "TCP__dport",
        "format": "integer",
        "encoding": "word_port"
      }
    ],
    "timeseries": [
      {
        "name": "IP__ttl",
        "format": "float",
        "encoding": "float"
      },
      {
        "name": "packet__len",
        "format": "float",
        "encoding": "float"
      },
      {
        "name": "DNS__answers",
        "format": "list",
        "abnormal": true,
        "encoding": "list_values",
        "names": {
          "name": "categorical",
          "type": "float",
          "cls": "float",
          "ttl": "float",
          "dlen": "float",
          "address": "categorical"
        },
        "delimiter": "="
      },
      {
        "name": "DNS__query",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "DNS__an",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "DNS__dlen",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "DNS__ttl",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "DNS__opcode",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "DNS__type",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "IEEE__type",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "IEEE__dsr",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "MQTT__mlen",
        "format": "float",
        "abnormal": true,
        "encoding": "float"
      },
      {
        "name": "TCP__flags",
        "format": "float",
        "encoding": "float"
      },
      {
        "name": "IP__len",
        "format": "float",
        "encoding": "float"
      },
      {
        "name": "TCP__seq",
        "format": "float",
        "encoding": "float"
      }
    ]
  }
}